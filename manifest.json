{
  "name": "Dall-e Mini Autorefresh",
  "description": "Keeps asking dall-e mini to create an image. - Made by @justekoro",
  "version": "0.1",
  "manifest_version": 3,
  "author": "justekoro",
  "background": {
    "service_worker": "autorefresh.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
}
